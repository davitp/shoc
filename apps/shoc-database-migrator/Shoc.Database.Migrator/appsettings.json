{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient.Eureka": "Warning",
      "Startup.Steeltoe.Discovery.Eureka": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Self": {
    "Application": "shoc-database-migrator",
    "ExternalBaseAddress": "http://localhost:11012/"
  },
  "Discovery": {
    "Primary": "static",
    "Fallback": null
  },
  "StaticDiscovery": {
    "Host": "localhost"
  },
  "GatewayDiscovery": {
    "Gateway": ""
  },
  "DataSource": {
    "ConnectionString": "Server=localhost;Port=11001;Uid=shoc;Pwd=shoc;",
    "Database": "shoc"
  },
  "Auth": {
    "Authority": "https://localhost:11017/",
    "Audience": "shoc-database-migrator",
    "AllowInsecure": true,
    "SkipIssuerValidation": false
  },
  "Client": {
    "ClientId": "machine",
    "ClientSecret": "machine",
    "Scope": "svc"
  },
  "Migration": {
    "MigrateOnStartup": true,
    "StopOnMigrate": true
  } 
}
